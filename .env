# .env.example for VoidBot Open-Source

# Discord bot token (keep this secret in your actual .env)
TOKEN=your_discord_bot_token_here

# Bot command prefix
PREFIX=!

# Optional: MongoDB connection string
MONGO_URI=mongodb://localhost:27017/voidbot

# Optional: Logging level (info, debug, warn, error)
LOG_LEVEL=info

# Optional: Bot owner ID
OWNER_ID=123456789012345678
